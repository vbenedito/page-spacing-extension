{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0.0",
  "description": "An extension to test content scripts.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Activate Extension"
  },
  "icons": {
    "16": "icon_24.png",
    "32": "icon_64.png",
    "64": "icon_64.png",
    "128": "icon_256.png"
  }
}
